<!DOCTYPE html>

{% include 'AppBundle:layout:header.html.twig' %}

{% stylesheets 'bundles/app/template/css/custom-home.css' filter='cssrewrite' %}
<link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}


<div id="parallax-slider-58e771280f371" class="parallax-slider">
    <ul class="baseList">
        {% image '@AppBundle/Resources/public/img/s1.jpg' %}
        <img src="{{ asset_url }}" alt="" class='img-responsive' />
        <li data-preview="{{ asset_url }}" data-thumb-url="{{ asset_url }}" data-ulr-link="">
            {% endimage %}
            <div class="slider_caption">
                <h2>Desarrollo</h2>
                <div class="subtitle-wrapper">
                    <h3>{% trans %}Desarrollo de apps, y páginas web{% endtrans %}</h3>
                </div>
                <a href="#" class="slider-button">{% trans %}Compra ahora!{% endtrans %}</a>
            </div>
        </li>

        {% image '@AppBundle/Resources/public/img/s2.jpg' %}
        <img src="{{ asset_url }}" alt="" class='img-responsive' />
        <li data-preview="{{ asset_url }}" data-thumb-url="{{ asset_url }}" data-ulr-link="">
            {% endimage %}
            <div class="slider_caption">
                <h2>Programación</h2>
                <div class="subtitle-wrapper">
                    <h3>{% trans %}Hola mundo!{% endtrans %}</h3>
                </div>
                <a href="#" class="slider-button">{% trans %}Compra ahora!{% endtrans %}</a>
            </div>
        </li>
    </ul>
</div>


<div class="motopress-wrapper content-holder clearfix">
    <div class="container">
        <div class="row">
            <div class="span12" data-motopress-wrapper-type="content">


                <div class="row">
                    <div class="span12" data-motopress-type="static">
                        <div class="slider_off"></div>
                    </div>
                </div>

                <div class="row">
                    <div class="span12" data-motopress-type="loop">
                        <div id="post-203" class="page post-203 type-page status-publish hentry">
                            <div class="banner_text_home">
                                <div class="banner_text_home_wrap_inner">
                                    <div class="content_box custom_home_banner_content ">
                                        <div class="row">
                                            {% for category in cat %}
                                                <div class="span3 visible-dk">
                                                    <section class="lazy-load-box " data-delay="200" data-speed="800" style="-webkit-transition: all 800ms ease; -moz-transition: all 800ms ease; -ms-transition: all 800ms ease; -o-transition: all 800ms ease; transition: all 800ms ease;">
                                                        <a href="#" title="{{category.name}}" class="banner-wrap ">
                                                            <figure class="featured-thumbnail">
                                                                {% if category.cover != null %}
                                                                    <img src="{{category.cover|vlabs_media('default')}}" title="{{category.name}}" alt=""/>
                                                                {% endif %}
                                                            </figure>
                                                            <h5>{{category.name}}</h5>
                                                            <div class="link-align banner-btn"></div>
                                                        </a>
                                                    </section>
                                                </div>
                                            {% endfor %}

                                            {# MOBILE 1 #}
                                            <div class="cherry_wc_product_carousel visible-mb" data-params='{"items":4, "itemsDesktop":[1199,4], "itemsDesktopSmall":[979,3], "itemsTablet":[768,2], "itemsMobile":[479,1], "navigation":true, "navigationText":["",""], "pagination":false}'>

                                                    <ul class="products owl-carousel slide-cat">
                                                        {% for category in cat %}
                                                        {# {% for item in prod %} #}
                                                            <li class="first post-1958 product type-product status-publish has-post-thumbnail product_cat-cpus product_cat-motherboards product_tag-branding product_tag-consulting product_tag-customer-service product_tag-ethics shipping-taxable purchasable product-type-simple product-cat-cpus product-cat-motherboards product-tag-branding product-tag-consulting product-tag-customer-service product-tag-ethics instock">

                                                                <div class="span3 ">
                                                                    <section class="lazy-load-box " data-delay="200" data-speed="800" style="-webkit-transition: all 800ms ease; -moz-transition: all 800ms ease; -ms-transition: all 800ms ease; -o-transition: all 800ms ease; transition: all 800ms ease;">
                                                                        <a href="#" title="{{category.name}}" class="banner-wrap ">
                                                                            <figure class="featured-thumbnail">
                                                                                {% if category.cover != null %}
                                                                                    <img src="{{category.cover|vlabs_media('default')}}" title="{{category.name}}" alt=""/>
                                                                                {% endif %}
                                                                            </figure>
                                                                            <h5>{{category.name}}</h5>
                                                                            <div class="link-align banner-btn">{% trans %}Compra ahora!{% endtrans %}</div>
                                                                        </a>
                                                                    </section>
                                                                </div>
                                                            </li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            {# MOBILE 0 #}
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="content_box home_text "></div>
                                </div>
                            </div>

                            <div class="featured_products">
                                <div class="featured_products_wrap_inner">
                                    <div class="row ">
                                        <div class="span12 ">
                                            <h2>{% trans %}Productos Destacados{% endtrans %}</h2>

                                            {# INICIO DE SLIDE DE PRODUCTOS #}
                                            <div class="woocommerce">
                                                <div class="cherry_wc_product_carousel " data-params='{"items":4, "itemsDesktop":[1199,4], "itemsDesktopSmall":[979,3], "itemsTablet":[768,2], "itemsMobile":[479,1], "navigation":true, "navigationText":["",""], "pagination":false}'>

                                                    <ul class="products owl-carousel">
                                                        {% for item in prod %}
                                                            <li class="first post-1958 product type-product status-publish has-post-thumbnail product_cat-cpus product_cat-motherboards product_tag-branding product_tag-consulting product_tag-customer-service product_tag-ethics shipping-taxable purchasable product-type-simple product-cat-cpus product-cat-motherboards product-tag-branding product-tag-consulting product-tag-customer-service product-tag-ethics instock">


                                                                <div class="cherry-thumb-wrap">
                                                                    {% image '@AppBundle/Resources/public/img/prod-default.png' %}
                                                                    <div class="producto-default" style="background-image: url({{ asset_url }});"></div>
                                                                    {% endimage %}
                                                                </div>
                                                                <h3>{{item.name}}</h3>
                                                                <div class="category-slg-c">
                                                                    <div class="category-logo" style="background-image: url({{item.category.cover|vlabs_media('default')}});"></div>
                                                                </div>
                                                                <span class="price">
                                                                    <span class="amount">{{item.priceFormated}}</span>
                                                                </span>
                                                                <div class="product-list-buttons" style="width: 100%;">
                                                                    <form action="{{path('producto')}}" method="POST">
                                                                        <input type="hidden" name="idd" value="{{item.id}}">
                                                                        <input type="submit" name="send"  value="{% trans %}Compra ahora!{% endtrans %}" style="width: 100%;margin-top: 5%;">
                                                                    </form>
                                                                    <div class="clear"></div>
                                                                </div>
                                                            </li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                                {# FIN CARRUSEL ITEMS #}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

